{
  "compilerOptions": {
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,

    "noEmitOnError": true,
    "moduleResolution": "node",
    "jsx": "react",
    "strict": true,
    "target": "es5",
    "module": "commonjs"

  }
}